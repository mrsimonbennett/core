{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "laravel/framework": "5.0.*",
    "rhumsaa/uuid": "~2.7",
    "broadway/broadway": "0.5.0",
    "graham-campbell/exceptions": "~2.0",
    "laravelcollective/annotations": "5.0.7",
    "cleentfaar/slack": "0.16.1",
    "stripe/stripe-php": "^2.0@dev",
    "gocardless/gocardless": "dev-master",
    "predis/predis": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "apigen/apigen": "~4.0@dev"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "FullRent\\Core\\Application\\": "app/",
      "FullRent\\Core\\": "src/"
    },
    "files": [
      "src/Infrastructure/Helpers/UuidHelper.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php",
      "tests/Specification.php"

    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
