{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "http://github.com/mrsimonbennett/laravel-doctrine"
    }
  ],
  "require": {
    "laravel/framework": "5.0.*",
    "rhumsaa/uuid": "~2.7",
    "mitchellvanw/laravel-doctrine": "dev-master",
    "broadway/broadway": "0.5.0",
    "graham-campbell/exceptions": "~1.0@dev",
    "dbellettini/eventstore-client": "@dev",
    "laravelcollective/annotations": "~5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "FullRent\\Core\\Application\\": "app/",
      "FullRent\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "php artisan clear-compiled",
      "php artisan optimize"
    ],
    "post-create-project-cmd": [
      "php -r \"copy('.env.example', '.env');\"",
      "php artisan key:generate"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
